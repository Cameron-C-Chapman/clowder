#!/usr/bin/env bash

cd "$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )" || exit 1

echo "Removing clowder.egg-info"
sudo -H rm -rf ../clowder_git.egg-info
echo "Removing dist"
sudo -H rm -rf ../dist
echo "Removing build"
sudo -H rm -rf ../build
echo "pip3 uninstall clowder"
sudo -H pip3 uninstall clowder-git
