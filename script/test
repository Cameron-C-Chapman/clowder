#!/usr/bin/env bash

if [ -z "$1" ]; then
    echo 'Requires parameter "cat" or "llvm"'
    exit 1
fi

cd "$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )" || exit 1

if [ $1 == "cats" ]; then
    ./tests/test_cats_example.sh
elif [ $1 == "llvm" ]; then
    ./tests/test_llvm_example.sh
else
    echo 'Requires parameter "cat" or "llvm"'
    exit 1
fi
